<h1>Listing resource_periods</h1>

<table>
  <tr>
    <th>resource name</th>
    <th>period name</th>
    <th>capacity</th>
    <th></th>
  </tr>

<% @resource_periods.each do |resource_period| %>
  <tr>
    <td><%= resource_period.resource.resource_name %></td>
    <td><%= resource_period.period.period_name %></td>
    <td><%= resource_period.capacity %></td>
    <td><%= link_to 'Show', resource_period %></td>
    <td><%= link_to 'Edit', edit_resource_period_path(resource_period) %></td>
    <td><%= link_to 'Destroy', resource_period, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

